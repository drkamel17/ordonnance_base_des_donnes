{
  "manifest_version": 2,
  "author": "Dr Daoudi",
  "homepage_url": "https://www.youtube.com/@bibmeddrkamel",
  "name": "Ordonnance et certificat avec base des donnes",
  "version": "1.22",
  "description": "Extension d'ordonnance",
  "browser_specific_settings": {
    "gecko": {
      "id": "certificat-base-donnees@drdaoudi.com",
      "data_collection_permissions": {
        "required": [
          "none"
        ]
      }
    }
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "nativeMessaging",
    "https://api.datamuse.com/*"
  ],
  "browser_action": {
    "default_title": "Ordonnance",
    "default_popup": "popup2.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_ui": {
    "page": "ord.html",
    "open_in_tab": true
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "templates/*",
    "styles/*",
    "medicaments.json",
    "medical_terms.json",
    "ordonnances-types.json",
    "dece/popup.html",
    "templates/popup-terms.html",
    "zagreb-certificat.html",
    "zagreb-certificat.js",
    "essen-certificat.html",
    "essen-certificat.js",
    "zagreb3-certificat.html",
    "zagreb3-certificat.js",
    "essen3-certificat.html",
    "essen3-certificat.js",
    "schema-tissulaire-sans-sar-certificat.html",
    "schema-tissulaire-sans-sar-certificat.js",
    "schema-tissulaire-avec-sar-certificat.html",
    "schema-tissulaire-avec-sar-certificat.js",
    "schema-risque-hemorragique-certificat.html",
    "schema-risque-hemorragique-certificat.js",
    "schema-risque-hemorragique-c2-certificat.html",
    "schema-risque-hemorragique-c2-certificat.js",
    "prophylaxie-pre-exposition-schema1-certificat.html",
    "prophylaxie-pre-exposition-schema1-certificat.js",
    "prophylaxie-pre-exposition-schema1-c2-certificat.html",
    "prophylaxie-pre-exposition-schema1-c2-certificat.js",
    "prophylaxie-pre-exposition-schema2-certificat.html",
    "prophylaxie-pre-exposition-schema2-certificat.js",
    "prophylaxie-pre-exposition-schema2-c2-certificat.html",
    "prophylaxie-pre-exposition-schema2-c2-certificat.js",
    "recuperer-donnees.html",
    "recuperer-donnees.js",
    "recuperer-donnees-scripts.js",
    "xlsx.full.min.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "rabique-save-handler.js",
        "antirabique-save-handler.js"
      ],
      "run_at": "document_start"
    }
  ]
}